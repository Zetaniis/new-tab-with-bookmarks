{
  "manifest_version": 2,
  "name": "New Tab with bookmarks",
  "version": "1.0",
  "description": "Replaces the new tab page with a grid of bookmarks from a chosen folder. Customizable layout and style. Vimium C friendly.",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "bookmarks",
    "storage"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "options_page": "options.html",
  "content_security_policy": "script-src 'self' chrome-extension://hfjbmagddngcpeloejdejnfgbamkjaeg/; object-src 'self'"
}
